<UserControl x:Class="LSLauncherWPF.View.UserControls.SBSPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LSLauncherWPF.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="3000" d:DesignWidth="1200">
    <Grid>
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel Orientation="Vertical" Margin="10">
                <Image Source="/Assets/Website/GameDevelopers/SBS.png" Stretch="UniformToFill" Height="Auto"/>
                <WrapPanel x:Name="GamesPanel" Width="1200"/>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
