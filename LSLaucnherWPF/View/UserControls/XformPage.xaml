<UserControl x:Class="LSLauncherWPF.View.UserControls.XformPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LSLauncherWPF.View.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="3000" d:DesignWidth="1200">
    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <StackPanel Orientation="Vertical" Margin="10">
            <Image Source="/Assets/Website/GameDevelopers/Xform.jpg" Stretch="Fill" Height="300"/>
            <WrapPanel x:Name="GamesPanel" Width="1200"/>
        </StackPanel>
    </ScrollViewer>
</UserControl>
